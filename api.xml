<?xml version="1.0"?>
<api>
	<description>
		<![CDATA[API for CampusAlerts]]>
	</description>
	
	<command id='crimelogs' security='public'>
	    <description><![CDATA[Interact with GTPD crime logs]]></description>
	    <index>
		    <description><![CDATA[Get all crime logs]]></description>
		    <action file='app.php'><![CDATA[getCrimeLogs();]]></action>
	    </index>
    </command>
    
    <command id='noncrimelogs' security='public'>
	    <description><![CDATA[Interact with GTPD non-crime logs]]></description>
	    <index>
		    <description><![CDATA[Get all non-crime logs]]></description>
		    <action file='app.php'><![CDATA[getNonCrimeLogs();]]></action>
	    </index>
    </command>
    
</api>
